# Installs the acme/ohmyzsh module from the Puppet Forge
mod 'acme/ohmyzsh', '0.1.2'

# Installs the puppetlabs/vcsrepo module from its GitHub repository
mod 'vcsrepo',
  :git => 'https://github.com/puppetlabs/puppetlabs-vcsrepo'

#Install epel 
mod 'stahnma-epel', '1.2.2'
  :git  =>  'https://github.com/stahnma/puppet-module-epel'

#Install module system for Linux
mod 'erwbgy-system', '0.7.4'
  :git  => 'https://github.com/erwbgy/puppet-system'

#Install Redis module
mod 'arioch-redis', '1.2.3'
  :git  => 'http://arioch.github.io/puppet-redis/'

#Install a module to handle sysctl variable values
mod 'thias-sysctl', '1.0.6'
  :git  =>  'https://github.com/thias/puppet-sysctl'




